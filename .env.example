# Variables de entorno para desarrollo local
# Copia este archivo a .env y ajusta los valores

# Google Cloud Project
GCP_PROJECT_ID=apolo-solutions-project
GCP_REGION=us-south1

# Google Cloud Storage
BUCKET_NAME=apolo-preavaluos-pdf-dev

# Credenciales (path local al archivo de service account)
GOOGLE_APPLICATION_CREDENTIALS=./credentials.json

# Cloud Run (para despliegue)
SERVICE_NAME=apolo-procesamiento-inteligente
SERVICE_ACCOUNT_EMAIL=apolo-procesamiento-sa@apolo-solutions-project.iam.gserviceaccount.com

# Firestore
FIRESTORE_PROJECT_ID=apolo-solutions-project

# Development
DEBUG=false
LOG_LEVEL=INFO
